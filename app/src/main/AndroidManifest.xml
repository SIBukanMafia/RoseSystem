<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="henrymenori.rosesystem" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PelayanActivity"
            android:label="@string/title_activity_pelayan" >
        </activity>
        <activity
            android:name=".StatusPesananActivity"
            android:label="@string/title_activity_status_pesanan" >
        </activity>
        <activity
            android:name=".KasirActivity"
            android:label="@string/title_activity_kasir" >
        </activity>
        <activity
            android:name=".StatusPembayaranActivity"
            android:label="@string/title_activity_status_pembayaran" >
        </activity>
        <activity
            android:name=".TambahPesananActivity"
            android:label="@string/title_activity_tambah_pesanan" >
        </activity>
        <activity
            android:name=".KokiActivity"
            android:label="@string/title_activity_koki" >
        </activity>
    </application>

</manifest>
